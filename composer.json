{
    "name": "bogordesain/lumen-enum",
    "description": "Elegant Enum implementation for Lumen. With make:enum command, ability to casting for Eloquent models and labels translations via lang files.",
    "type": "library",
    "keywords": [
        "bogordesain",
        "enum",
        "lumen-enum"
    ],
    "homepage": "https://github.com/bogordesaincom/lumen-enum.git",
    "license": "MIT",
    "authors": [
        {
            "name": "bogordesain",
            "email": "hello@bogordesain.com",
            "homepage": "https://github.com/bogordesaincom",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.3 || ^8.0" || ^8.1",
        "illuminate/console": "^7.0 || ^8.0 || ^9.0 || ^10.0",
        "illuminate/support": "^7.0 || ^8.0 || ^9.0 || ^10.0",
        "illuminate/translation": "^7.0 || ^8.0 || ^9.0 || ^10.0",
        "myclabs/php-enum": "^1.7.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.0.0",
        "orchestra/testbench": "^5.0 || ^6.0",
        "phpunit/phpunit": "^8.0 || ^9.3"
    },
    "autoload": {
        "psr-4": {
            "Bogordesain\\Enum\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Bogordesain\\Enum\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Bogordesain\\Enum\\EnumServiceProvider"
            ]
        }
    }
}
